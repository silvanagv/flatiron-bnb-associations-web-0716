<h1>Listing Form</h1>

<%=form_for(@listing) do |f| %>
  <label>Title</label>
  <%= f.text_field :title %>
  <label>Description</label>

  <%= f.text_field :description %>
  <label>Address</label>

  <%= f.text_field :address %>
<label>Listing Type</label>

  <%= f.radio_button :listing_type, "private room" %>
  <%= label_tag(:listing_type, ) %>
  
  <%= f.text_field :price %>

  <%=f.submit %>
<% end %>
